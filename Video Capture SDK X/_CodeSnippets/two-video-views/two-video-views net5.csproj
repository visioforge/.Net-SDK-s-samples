<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFrameworks>net5.0-windows</TargetFrameworks>
    <PlatformTarget>x64</PlatformTarget>
    <RootNamespace>two_video_views</RootNamespace>
    <UseWindowsForms>true</UseWindowsForms>
    <ApplicationIcon>visioforge_main_icon.ico</ApplicationIcon>
  </PropertyGroup>
  <ItemGroup>
    <Content Include="visioforge_main_icon.ico" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="VisioForge.DotNet.VideoCapture" Version="2025.7.15" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="VisioForge.CrossPlatform.Core.Windows.x64" Version="2025.7.10" />
    <PackageReference Include="VisioForge.CrossPlatform.Libav.Windows.x64" Version="2025.7.10" />
  </ItemGroup>
</Project>