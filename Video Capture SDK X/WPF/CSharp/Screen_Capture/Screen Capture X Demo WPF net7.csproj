<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net7.0-windows</TargetFramework>
    <RootNamespace>Screen_Capture_X</RootNamespace>
    <UseWPF>true</UseWPF>
    <PlatformTarget>x64</PlatformTarget>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="visioforge_main_icon.ico" />
  </ItemGroup>
  <ItemGroup>
    <Resource Include="visioforge_main_icon.ico" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="VisioForge.CrossPlatform.Core.Windows.x64" Version="2025.0.25" />
    <PackageReference Include="VisioForge.CrossPlatform.Libav.Windows.x64" Version="2025.0.25" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="VisioForge.DotNet.VideoCapture" Version="2025.1.2" />
    <PackageReference Include="VisioForge.DotNet.Core.UI.WPF" Version="2025.1.2" />
    <PackageReference Include="VisioForge.DotNet.Core.UI.WinForms" Version="2025.1.2" />
  </ItemGroup>
</Project>