<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <RootNamespace>TV_Tuner_Demo</RootNamespace>
    <AssemblyName>TV Tuner Demo</AssemblyName>
    <TargetFrameworks>net5.0-windows</TargetFrameworks>
    <RuntimeIdentifiers>win7-x64;win7-x86</RuntimeIdentifiers>
    <AssemblyTitle>TV Tuner Demo</AssemblyTitle>
    <Product>TV Tuner Demo</Product>
    <Copyright>Copyright (c) 2006-2023</Copyright>
    <AssemblyVersion>15.5.29.0</AssemblyVersion>
    <FileVersion>15.5.29.0</FileVersion>
    <OutputPath>bin\$(Configuration)\</OutputPath>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <DebugType>full</DebugType>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <DebugType>pdbonly</DebugType>
  </PropertyGroup>
  <PropertyGroup>
    <ApplicationIcon>visioforge_main_icon.ico</ApplicationIcon>
  </PropertyGroup>
  <ItemGroup Condition=" '$(TargetFramework)' == 'net472'">
    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System.Net.Http" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="VisioForge.DotNet.VideoCapture" Version="15.5.29" />
    <PackageReference Include="VisioForge.DotNet.Core.Redist.VideoCapture.x64" Version="15.5.29" />
    <PackageReference Include="VisioForge.DotNet.Core.Redist.VideoCapture.x86" Version="15.5.29" />
    <PackageReference Include="VisioForge.DotNet.Core.Redist.MP4.x64" Version="15.5.29" />
    <PackageReference Include="VisioForge.DotNet.Core.Redist.MP4.x86" Version="15.5.29" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="visioforge_main_icon.ico" />
  </ItemGroup>
</Project>
