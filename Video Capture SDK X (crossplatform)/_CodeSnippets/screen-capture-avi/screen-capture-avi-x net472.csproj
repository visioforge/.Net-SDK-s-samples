<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFrameworks>net472</TargetFrameworks>
    <PlatformTarget>x64</PlatformTarget>
    <RootNamespace>screen_capture_avi</RootNamespace>
    <UseWindowsForms>true</UseWindowsForms>
    <ApplicationIcon>visioforge_main_icon.ico</ApplicationIcon>
  </PropertyGroup>
  <ItemGroup>
    <Content Include="visioforge_main_icon.ico" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="VisioForge.CrossPlatform.Core.Windows.x64" Version="15.10.31" />
    <PackageReference Include="VisioForge.CrossPlatform.Libav.Windows.x64" Version="15.10.31" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="VisioForge.DotNet.VideoCapture" Version="15.10.32" />
    <PackageReference Include="VisioForge.DotNet.Core.UI.WinForms" Version="15.10.32" />
  </ItemGroup>
</Project>
