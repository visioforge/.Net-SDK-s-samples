<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net10.0-windows</TargetFramework>
    <RootNamespace>Video_From_Images</RootNamespace>
    <PlatformTarget>x64</PlatformTarget>
    <UseWPF>true</UseWPF>
    <UseWindowsForms>true</UseWindowsForms>
    <ApplicationIcon>visioforge_main_icon.ico</ApplicationIcon>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="visioforge_main_icon.ico" />
  </ItemGroup>
  <ItemGroup>
    <Resource Include="visioforge_main_icon.ico" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="VisioForge.DotNet.VideoEdit" Version="2026.2.11" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Images\1.jpg" />
    <EmbeddedResource Include="Images\2.jpg" />
    <EmbeddedResource Include="Images\3.jpg" />
    <EmbeddedResource Include="Images\4.jpg" />
    <EmbeddedResource Include="Images\5.jpg" />
  </ItemGroup>
</Project>