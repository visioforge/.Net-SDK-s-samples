<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net7.0-windows</TargetFramework>
    <RootNamespace>Cut_Video_File</RootNamespace>
    <UseWPF>true</UseWPF>
    <Platform>x64</Platform>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="visioforge_main_icon.ico" />
  </ItemGroup>
  <ItemGroup>
    <Resource Include="visioforge_main_icon.ico" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="VisioForge.DotNet.VideoEdit" Version="2025.9.11" />
    <PackageReference Include="VisioForge.DotNet.Core.Redist.VideoEdit.x64" Version="2025.9.11" />
    <PackageReference Include="VisioForge.DotNet.Core.Redist.FFMPEG.x64" Version="2025.9.11" />
  </ItemGroup>
</Project>