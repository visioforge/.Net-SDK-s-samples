<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <RuntimeIdentifiers>win7-x64</RuntimeIdentifiers>
    <PlatformTarget>x64</PlatformTarget>
    <RootNamespace>Multiple_Audio_Tracks_Demo</RootNamespace>
    <UseWPF>true</UseWPF>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="visioforge_main_icon.ico" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="VisioForge.DotNet.VideoEdit" Version="2025.1.3" />
    <PackageReference Include="VisioForge.DotNet.Core.UI.WPF" Version="2025.1.3" />
    <PackageReference Include="VisioForge.DotNet.Core.Redist.VideoEdit.x64" Version="2025.1.3" />
    <PackageReference Include="VisioForge.DotNet.Core.Redist.MP4.x64" Version="2025.1.3" />
    <PackageReference Include="VisioForge.DotNet.Core.Redist.LAV.x64" Version="2025.1.3" />
  </ItemGroup>
  <ItemGroup>
    <Resource Include="visioforge_main_icon.ico" />
  </ItemGroup>
</Project>