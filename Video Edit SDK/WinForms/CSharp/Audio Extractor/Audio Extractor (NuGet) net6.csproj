<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <PlatformTarget>x64</PlatformTarget>
    <RootNamespace>Audio_Extractor</RootNamespace>
    <Nullable>disable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ApplicationIcon>visioforge_main_icon.ico</ApplicationIcon>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="visioforge_main_icon.ico" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="visioforge_main_icon.ico" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="VisioForge.DotNet.VideoEdit" Version="2025.12.6" />
    <PackageReference Include="VisioForge.DotNet.Core.Redist.VideoEdit.x64" Version="2025.12.6" />
    <PackageReference Include="VisioForge.DotNet.Core.Redist.MP4.x64" Version="2025.12.6" />
    <PackageReference Include="VisioForge.DotNet.Core.Redist.LAV.x64" Version="2025.12.6" />
    <PackageReference Include="VisioForge.DotNet.Core.Redist.FFMPEG.x64" Version="2025.12.6" />
  </ItemGroup>
</Project>